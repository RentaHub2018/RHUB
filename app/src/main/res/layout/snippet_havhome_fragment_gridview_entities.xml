<merge xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <TextView
            android:textSize="20dp"
            android:layout_marginHorizontal="@dimen/activity_horizontal_margin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textviewamenities"
            android:text="Amenities"/>
        <GridLayout
            android:padding="20dp"
            android:layout_marginTop="20dp"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rowCount="3"
            android:columnCount="3"
            android:layout_below="@id/textviewamenities"
            >
            <RelativeLayout
                android:padding="10dp"
                android:clickable="true"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutTvItem"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_tv_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tv"
                    android:layout_marginLeft="25dp"
                    android:layout_below="@id/myGridLayoutTvItem"
                    android:textSize="15dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutFridgeItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_fridge_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="fridge"
                    android:textSize="15dp"
                    android:layout_below="@id/myGridLayoutFridgeItem"
                    android:layout_marginLeft="20dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutKitchenItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_kitchen_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutKitchenItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Kitchen"
                    android:textSize="15dp"
                    android:layout_marginLeft="15dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutWifiItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_wifi_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutWifiItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Wifi"
                    android:textSize="15dp"
                    android:layout_marginLeft="25dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutWasherMachineItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_laundry_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutWasherMachineItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="machine"
                    android:textSize="15dp"
                    android:layout_marginLeft="10dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutAcItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_fan_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutAcItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ac"
                    android:textSize="15dp"
                    android:layout_marginLeft="25dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutFurnitureItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_bed_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutFurnitureItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Furniture"
                    android:textSize="15dp"
                    android:layout_marginLeft="10dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutCarItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_car_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutCarItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Parking"
                    android:textSize="15dp"
                    android:layout_marginLeft="10dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:padding="10dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:orientation="vertical">
                <ImageButton
                    android:id="@+id/myGridLayoutHandicamItem"
                    android:layout_marginLeft="10dp"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:src="@drawable/ic_handicapped_black"
                    android:background="@color/colorTransparent"/>
                <TextView
                    android:layout_below="@id/myGridLayoutHandicamItem"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handicap"
                    android:textSize="15dp"
                    />
            </RelativeLayout>
        </GridLayout>
    </RelativeLayout>
</merge>